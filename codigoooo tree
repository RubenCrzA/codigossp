                tabla += '</tr>';

                $(subTablaData).each(function(j, subitem) {
                    if (subitem.idCampania === item.idCampania) {
                        tabla += '<tr class="subfila" data-parent-id="'+item.idCampania+'" id="subfila-'+subitem.idCampania+'-'+j+'" style="cursor: pointer;">';
