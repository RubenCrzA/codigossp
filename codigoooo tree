    function cargarSubTabla(parentId) {
        var subFilas = subTablaData.filter(function(item) {
            return item.idCampania === parentId
    });

        var subTabla = '';
        $(subFilas).each(function(j, subitem) {
            var color = subitem.color == "Rojo" ? "red" : "#20c820";
            subTabla += '<tr id="subfila-'+subitem.idCampania+'-'+j+'" class="subfila" data-parent-id="'+parentId+'" style="cursor: pointer;">';
            subTabla += '<td style="padding-left: 20px;">'+subitem.desEjecutivo+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.numClientes+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.montoOfertado+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.gestionado+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.pctGestionado+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.noInteresado+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.noContactable+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.agenda+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.pipeline+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.cierreCantado+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.cierreReal+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.pctCierreReal+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.gestionGteTx+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.gestionAgro+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.gestionGetnet+'</td>';
            subTabla += '<td class="estiloTabla" style="color: '+color+'">'+subitem.gestionRof+'</td>';
            subTabla += '</tr>';
        });

        $('#tablaCampTbody2Credito').append(subTabla);
    }

    function cargarSubSubTabla(parentId) {
        var subSubFilas = subSubTablaData.filter(function(item) {
            return item.idSubgrupo === parentId;
        });

        var subSubTabla = '';
        $(subSubFilas).each(function(k, subsubitem) {
            var color = subsubitem.color == "Rojo" ? "red" : "#20c820";
            subSubTabla += '<tr class="subfila-nivel2" data-parent-id="subfila-'+parentId+'" style="padding-left: 40px;">';
            subSubTabla += '<td>'+subsubitem.desEjecutivo+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.numClientes+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.montoOfertado+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.gestionado+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.pctGestionado+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.noInteresado+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.noContactable+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.agenda+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.pipeline+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.cierreCantado+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.cierreReal+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.pctCierreReal+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.gestionGteTx+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.gestionAgro+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.gestionGetnet+'</td>';
            subSubTabla += '<td class="estiloTabla" style="color: '+color+'">'+subsubitem.gestionRof+'</td>';
            subSubTabla += '</tr>';
        });

        $('#tablaCampTbody2Credito').append(subSubTabla);
    }

    crearTabla(lstCampanias);

    $(document).on('click', '.filaPrincipal', function() {
        var parentId = $(this).attr('id');
        $('tr[data-parent-id]').hide(); // Oculta todas las subfilas y subsubfilas
        $('tr[data-parent-id="'+parentId+'"]').remove(); // Elimina las subfilas cargadas anteriormente
        cargarSubTabla(parentId);
    });

    $(document).on('click', '.subfila', function() {
        var parentId = $(this).attr('id');
        $('tr[data-parent-id^="subfila-"]').hide(); // Oculta todas las subsubfilas
        $('tr[data-parent-id="'+parentId+'"]').remove(); // Elimina las subsubfilas cargadas anteriormente
        cargarSubSubTabla(parentId);
    });
});




















			
